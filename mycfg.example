[general]
ann_path=pascal/VOCdevkit/annex
# path from which to load pascal yolo weights
ckpt_path=ckpt/yolo-tiny-664.meta
labels=un deux trois
exclusive=True
epoch=1000
learning_rate=1e-5
# save every iter
save_iter=10000

[net]
batch=64
subdivisions=64
height=448
width=448
channels=3
momentum=0.9
decay=0.0005

learning_rate=0.0001
policy=steps
steps=20,40,60,80,20000,30000
scales=5,5,2,2,.1,.1
max_batches=40000

[detection]
threshold=0.1
classes=12
coords=4
rescore=1
side=7
num=2
softmax=0
sqrt=1
jitter=.2
object_scale=1
noobject_scale=.5
class_scale=1
coord_scale=5
